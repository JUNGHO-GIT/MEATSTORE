	"void_elements": [
    "!doctype",
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr",
    "path",
    "circle",
    "ellipse",
    "line",
    "rect",
    "use",
    "stop"
  ],
	"extra_liners": [
    "!doctype",
    "head",
    "body",
    "/body"
  ],
	"templating": [
    "auto"
  ]
}
